{
	"name": "@example/minimal",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build --remote",
		"build:force-db": "WITH_DB=true astro build --remote",
		"preview": "astro preview",
		"lint": "biome lint .",
		"format": "pnpm run format:code && pnpm run format:imports",
		"format:code": "biome format . --write && prettier -w \"**/*\" \"!**/*.{md,mdx}\" --ignore-unknown --cache",
		"format:imports": "biome check --formatter-enabled=false --write",
		"format-unsafe": "biome format . --write --unsafe",
		"typecheck": "astro check",
		"update:integrations": "node -r dotenv/config ./scripts/update-integrations.mjs",
		"update:showcase": "node -r dotenv/config ./scripts/update-showcase.mjs",
		"audit:showcase": "node -r dotenv/config .scripts/audit-showcase.mjs"
	},
	"dependencies": {
		"@astro-community/astro-embed-youtube": "^0.5.3",
		"@astrojs/db": "^0.11.4",
		"@astrojs/mdx": "^3.1.3",
		"@astrojs/netlify": "^5.5.1",
		"@astrojs/rss": "^4.0.7",
		"@astrojs/sitemap": "^3.1.6",
		"@astrojs/solid-js": "^4.4.1",
		"@astrojs/tailwind": "^5.1.0",
		"@astrojs/web-vitals": "^0.2.1",
		"@iconify-json/ri": "^1.1.20",
		"astro": "^4.14.2",
		"astro-expressive-code": "^0.35.6",
		"chart.js": "4.4.1",
		"cobe": "^0.6.3",
		"motion": "^10.17.0",
		"slugify": "^1.6.6",
		"solid-js": "^1.8.21",
		"tailwindcss": "^3.4.10",
		"xss": "^1.0.15"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.3",
		"@astrojs/site-kit": "github:withastro/site-kit",
		"@biomejs/biome": "1.8.3",
		"@types/json-to-pretty-yaml": "1.2.1",
		"@types/node": "^20.10.4",
		"astro-icon": "^0.8.2",
		"date-fns": "^2.30.0",
		"dotenv": "^16.3.1",
		"json-to-pretty-yaml": "^1.2.2",
		"mdast-util-from-markdown": "^2.0.1",
		"mdast-util-to-string": "^4.0.0",
		"only-allow": "1.2.1",
		"prettier": "^3.3.3",
		"prettier-plugin-astro": "^0.14.1",
		"smartypants": "^0.2.2",
		"tiny-glob": "^0.2.9",
		"typescript": "^5.5.4"
	},
	"optionalDependencies": {
		"@actions/core": "^1.10.1",
		"@octokit/graphql": "^7.0.2",
		"gray-matter": "^4.0.3",
		"linkedom": "^0.16.4",
		"puppeteer": "^21.6.0",
		"sharp": "^0.33.5"
	},
	"packageManager": "pnpm@9.4.0"
}
