{
	"name": "@example/minimal",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"lint": "biome lint",
		"format": "pnpm run format:code && pnpm run format:imports",
		"format:code": "biome format --write && prettier -w \"**/*\" \"!**/*.{md,mdx}\" --ignore-unknown --cache",
		"format:imports": "biome check --formatter-enabled=false --linter-enabled=false --write",
		"format-unsafe": "biome check --write --unsafe",
		"typecheck": "astro check",
		"update:integrations": "node -r dotenv/config ./scripts/update-integrations.mjs",
		"update:showcase": "node -r dotenv/config ./scripts/update-showcase.mjs",
		"audit:showcase": "node -r dotenv/config ./scripts/audit-showcase.mjs"
	},
	"dependencies": {
		"@astro-community/astro-embed-youtube": "^0.5.7",
		"@astrojs/check": "^0.9.4",
		"@astrojs/mdx": "^4.3.7",
		"@astrojs/netlify": "^6.5.13",
		"@astrojs/rss": "^4.0.12",
		"@astrojs/sitemap": "^3.6.0",
		"@astrojs/tailwind": "^6.0.2",
		"@biomejs/biome": "2.2.6",
		"@netlify/plugin-lighthouse": "^6.0.3",
		"@preact/signals-core": "^1.12.1",
		"@tailwindcss/container-queries": "^0.1.1",
		"@types/node": "^22.18.11",
		"astro": "^5.14.6",
		"astro-embed": "^0.9.1",
		"astro-expressive-code": "^0.41.2",
		"astro-icon": "^1.1.5",
		"chart.js": "4.4.8",
		"date-fns": "^4.1.0",
		"dotenv": "^17.2.3",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"sharp": "^0.34.4",
		"smartypants": "^0.2.2",
		"tailwindcss": "^3.4.18",
		"typescript": "^5.9.3",
		"xss": "^1.0.15"
	},
	"devDependencies": {
		"@actions/core": "^1.11.1",
		"@octokit/graphql": "^7.1.1",
		"gray-matter": "^4.0.3",
		"linkedom": "^0.18.12",
		"mdast-util-from-markdown": "^2.0.2",
		"mdast-util-to-string": "^4.0.0",
		"p-limit": "^7.1.1",
		"p-retry": "^7.1.0",
		"puppeteer": "^21.11.0",
		"slugify": "^1.6.6",
		"tiny-glob": "^0.2.9",
		"yaml": "^2.8.1"
	},
	"packageManager": "pnpm@10.18.3"
}
