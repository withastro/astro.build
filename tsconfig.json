{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "importsNotUsedAsValues": "remove"
  },
  "include": ["**/*", ".eslintrc.cjs"],
  "exclude": ["node_modules", "dist", ".vercel", "public"]
}
