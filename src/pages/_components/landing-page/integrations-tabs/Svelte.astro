---
import { Content } from './snippets/svelte.mdx';
import InterfaceExample from "./InterfaceExample.astro";
import IntegrationContent from "../IntegrationContent.astro";

const { position } = Astro.props
---
<IntegrationContent tab="svelte" position={position} class="hidden">
    <Content slot="code" />
    <InterfaceExample slot="example" brandColor="bg-[#FF3E00]" />
</IntegrationContent>
