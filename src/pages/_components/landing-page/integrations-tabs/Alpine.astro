---
import { Content } from './snippets/jsx.mdx';
import InterfaceExample from "./InterfaceExample.astro";
import IntegrationContent from "../IntegrationContent.astro";

const { position } = Astro.props
---
<IntegrationContent tab="alpine" position={position} class="hidden">
    <Content slot="code" />
    <InterfaceExample slot="example" brandColor="bg-[#77C1D2]" highContrast />
</IntegrationContent>