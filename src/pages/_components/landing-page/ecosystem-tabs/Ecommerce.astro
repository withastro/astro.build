const themes = [
	{
		href: '/themes/details/astro-shopify/',
		title: 'Astro Shopify',
		author: 'thomasKn',
		src: astroShopify,
		isSeenOnMobile: true,
	},
	{
		href: '/themes/details/quick-store/',
		title: 'Quick Store',
		author: 'Lexington Themes',
		src: quickStore,
		isSeenOnMobile: true,
